        "symfony/dom-crawler"       : "dev-master",
        "symfony/browser-kit"       : "dev-master"
    },
    
    "require" : {
        "icomefromthenet/dbal-gateway" : "dev-master",
        "icomefromthenet/laterjob"     : "dev-master",
        "icomefromthenet/quicktag"     : "dev-master",  
